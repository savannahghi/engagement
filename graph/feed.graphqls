# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type LibraryItem {
  title: String!
  description: String!
}

type FAQ {
  question: String!
  answer: String!
}

type FeedItem {
  title: String!
  description: String!

}

type Query {
  getLibraryContent: [LibraryItem!]
  getFaqs: [FAQ!]
  getFeedItems: [FeedItem!]
}
